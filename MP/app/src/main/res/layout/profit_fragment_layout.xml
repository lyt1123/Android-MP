<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f3f3f3"
    android:gravity="center">

    <ListView
        android:id="@+id/listview_profit"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="#000000"
        android:dividerHeight="0dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="分润"
        android:textColor="#fff"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profit_statistic_tv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="30dp"
        android:text="收益透明零差异"
        android:textSize="10dp"
        android:textColor="#999999"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/profit_statistic_img"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/profit_statistic_tv"
        android:layout_marginBottom="8dp"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:src="@drawable/profit_statistic"/>

    <TextView
        android:id="@+id/profit_city_tv"
        app:layout_constraintRight_toLeftOf="@+id/profit_statistic_tv"
        android:layout_marginRight="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="30dp"
        android:text="总部直联零门槛"
        android:textSize="10dp"
        android:textColor="#999999"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/profit_city_img"
        app:layout_constraintLeft_toLeftOf="@+id/profit_city_tv"
        app:layout_constraintRight_toRightOf="@id/profit_city_tv"
        app:layout_constraintBottom_toTopOf="@+id/profit_city_tv"
        android:layout_marginBottom="8dp"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:src="@drawable/profit_city"/>

    <TextView
        android:id="@+id/profit_safe_tv"
        app:layout_constraintLeft_toRightOf="@+id/profit_statistic_tv"
        android:layout_marginLeft="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="30dp"
        android:text="总部直联零门槛"
        android:textSize="10dp"
        android:textColor="#999999"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/profit_safe_img"
        app:layout_constraintLeft_toLeftOf="@+id/profit_safe_tv"
        app:layout_constraintRight_toRightOf="@id/profit_safe_tv"
        app:layout_constraintBottom_toTopOf="@+id/profit_safe_tv"
        android:layout_marginBottom="8dp"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:src="@drawable/profit_safe"/>

</androidx.constraintlayout.widget.ConstraintLayout>
